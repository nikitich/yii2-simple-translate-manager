{
  "name": "nikitich/yii2-translate-manager",
  "description": "Translation management extension for Yii 2",
  "type": "yii2-extension",
  "keywords": ["yii2", "extension", "translate", "language", "module"],
  "license": "MIT",
  "authors": [
    {
      "name": "nikitich",
      "email": "nickotin.zp.ua@gmail.com"
    }
  ],
  "require": {
    "yiisoft/yii2": "~2.0.0",
    "yiisoft/yii2-jui": "~2.0.0",
    "yiisoft/yii2-bootstrap": "~2.0.0",
    "kartik-v/yii2-grid": "9999999-dev",
    "kartik-v/yii2-widget-select2": "9999999-dev",
    "kartik-v/yii2-tabs-x": "9999999-dev"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "nikitich\\simpletranslatemanager\\": ""
    }
  },
  "scripts": {
    "cs-fix": "php-cs-fixer fix --config=.php_cs",
    "cs-fix-dry-run": "php-cs-fixer fix --config=.php_cs --dry-run --diff"
  }
}
